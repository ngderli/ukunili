<!DOCTYPE html><html><head>  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>  <%= csrf_meta_tags %></head><!--<body background="Background_1.jpg">--><body><div class="content"><div id="fb-root"></div><script>(function(d, s, id) {  var js, fjs = d.getElementsByTagName(s)[0];  if (d.getElementById(id)) return;  js = d.createElement(s); js.id = id;  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.4";  fjs.parentNode.insertBefore(js, fjs);}(document, 'script', 'facebook-jssdk'));</script><div id="header">	<div class ='header_div'>		<header>  			<%= link_to(image_tag("Ukunili_Logo.png",:size => '150x125',class:"Ukunili_Logo"),root_url) %>		</header>	</div>	<div class='nav_top'>		<nav class='nav_up'>			<ul class="nav_left">  				<li><%= link_to("Home",root_url)%></li>  				<li><%= link_to("Blog","http://www.ukunili.com")%></li> 				<li><%= link_to("Ukulele",static_construction_path)%></li>  				<li><%= link_to("Accessories",static_construction_path)%></li>			</ul>			<ul class="nav_right">    			<li><%= link_to("Login",static_construction_path)%></li>			</ul>		</nav>	</div>	<div class='nav_down'>		<h1>Pick me up & Play me!!</h1>	</div></div><%= yield %><div id='footer'>	<div class='footer_box'>		<div class='f_up'>			<div class='f_up1'>				<h4>				Our Company				</h4>			</div>			<div class='f_up1'>				<h4>				Ukunili Branch				</h4>			</div>			<div class='f_up1'>				<h4>				Shopping Info				</h4>			</div>			<div class='f_up1'>				<h4>				Related Ukunili Sites				</h4>			</div>			<div class='f_up1'>				<h4>				Follow Us				</h4>			</div>		</div>		<div class='f_down'>		<!--Our Company-->			<div class="f_down1">				<%= link_to("About Ukunili",static_about_path)%>				<%= link_to("Contact Us",static_contact_us_path)%>			</div>		<!--Ukunili Brance-->			<div class="f_down1">				<%= link_to("Sutera Mall L1-205",static_contact_us_path)%>				<%= link_to("KSL City Mall L1-136",static_contact_us_path)%>			</div>		<!--Others-->			<div class="f_down1">				<%= link_to("Message us at Facebook","https://www.facebook.com/ukunili")%>			</div>		<!--Related Ukunili Sites-->			<div class="f_down1">				<%= link_to("Victoria Music Studio","http://www.victoriamusic.my")%><br>				<%= link_to "#{image_tag("icon-facebook.png",:size => '15x15',:alt =>"Facebook")}Facebook".html_safe,"https://www.facebook.com/ukunili"%>				<%= link_to("#{image_tag("icon-instagram.png",:size=>'15x15',:alt=>"Instagram")}Instagram".html_safe,"https://instagram.com/ukunili/")%>				<%= link_to("#{image_tag("icon-google.png",:size=>'15x15',:alt=>"Google+")}Google+".html_safe,"https://plus.google.com/+UkuniliUkuleleSkudai")%>			</div>			<div class="fb-page" data-href="https://www.facebook.com/ukunili" data-width="360" data-height="360" data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true" data-show-posts="true"><div class="fb-xfbml-parse-ignore"><blockquote cite="https://www.facebook.com/ukunili"><a href="https://www.facebook.com/ukunili">Ukunili ukuleles</a></blockquote></div></div>		<footer>			<p>&copy; 2014 Ukunili Ukulele Trading. All rights reserved.</p>		</footer>	</div></div></div></body></html>